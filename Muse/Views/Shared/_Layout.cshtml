<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>

    <link rel="apple-touch-icon" sizes="180x180" href="~/favicon_io/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/favicon_io/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/favicon_io/favicon-16x16.png">
    <link rel="manifest" href="~/favicon_io/site.webmanifest">

    <link rel="stylesheet" href="~/css/Site.css" type="text/css" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <header>
        <a class="link" asp-area="" asp-controller="Users" asp-action="Frontpage">
            <h1 id="frontpagelink">MUSE</h1>
        </a>

        <!-- <p id="greeting">Good Day Human!</p> -->
        <a id="settings-link" class="link" asp-area="" asp-controller="Users" asp-action="Details">
            <h6 id="settings-border">MY ACCOUNT</h6>
        </a>
    </header>
    <p class="error"><strong> @TempData["Error"] </strong></p>
    @RenderBody()

    @RenderSection("Scripts", required: false)
</body>
</html>
